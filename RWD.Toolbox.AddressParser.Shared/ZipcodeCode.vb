''' <summary>
''' Entity DTO for Postal Zipcodes
''' </summary>
Public Class ZipcodeCode

    ''' <summary>
    '''  Unique Identifer for Zipcode
    ''' </summary>
    ''' <returns></returns>
    Public Property Id As Integer?

    ''' <summary>
    '''  Postal Zipcode
    ''' </summary>
    ''' <returns></returns>
    Public Property Zipcode As String

    ''' <summary>
    '''  State Code Associated with Zip
    ''' </summary>
    ''' <returns></returns>
    Public Property City As String

    ''' <summary>
    '''  State Code Associated with Zip
    ''' </summary>
    ''' <returns></returns>
    Public Property StateCode As String

    ''' <summary>
    '''  Area Code Associated with Zip
    ''' </summary>
    ''' <returns></returns>
    Public Property AreaCode As String

    ''' <summary>
    '''  County Associated with Zip
    ''' </summary>
    ''' <returns></returns>
    Public Property County As String

    ''' <summary>
    '''  Latitude Associated with Zip
    ''' </summary>
    ''' <returns></returns>
    Public Property Latitude As String

    ''' <summary>
    '''  Longitute Associated with Zip
    ''' </summary>
    ''' <returns></returns>
    Public Property Longitude As String

End Class
